Q1=Q1
Q2=Q2
Q3=Q3
clear
echo "---------make---------"
make

if [ "$Q1"!="N" ]; then
	echo "----------Q1----------"
	./$Q1 <<< 	"5
			12
			13"
	./$Q1 <<< 	"20
			14
			34"
	./$Q1 <<< 	"6
			7
			8"
	./$Q1 <<< 	"2
			3
			4"
fi

if [ "$Q2"!="N" ]; then
	echo "----------Q2----------"
	./$Q2
fi

if [ "$Q3"!="N" ]; then
	echo "----------Q3----------"
	./$Q3 <<< "0.2024
			1
			2"
	./$Q3 <<< "0.0307
			2
			2"
	./$Q3 <<< "0.2024
			1
			0"
	./$Q3 <<< "0.0307
			2
			2"
fi

echo "------make clean------"
make clean
